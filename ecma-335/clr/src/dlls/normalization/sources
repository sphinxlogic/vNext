NO_NTDLL    = 1

TARGETNAME  = normalization
TARGETPATH  = $(_OBJ_DIR)
TARGETTYPE  = DYNLINK

UMTYPE      = windows
DLLDEF      = normalization.def
DLLENTRY    = _DllMainCRTStartup

DLLENTRY = _DllMainCRTStartup

SOURCES= \
    native.rc               \
    helpers.cpp             \
    normalizationDLL.cpp    \
    normalization.cpp       \


