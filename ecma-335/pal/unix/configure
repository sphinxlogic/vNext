#include <stdio.h>

int main(void)
{
    char* szFileName;
    FILE* pFile = NULL;
    int ret = 0;

    szFileName = tempnam(".", "tmp");

    /* open the file write-only */
    pFile = fopen(szFileName, "a");
    if (pFile == NULL)
    {
        exit(1);
    }
    if (ungetc('A', pFile) != EOF)
    {
        ret = 1;
    }
    unlink(szFileName);
    exit(ret);
