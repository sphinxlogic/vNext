#include <vcc.h>

void defaultClassifs()
{
	int h;
	int x = 0, y = 0;

	if (h  > 0) x = 1;
	
	assert(is_low(y));
	x = 0;
	assert(is_low(x));
}

void defaultClassifsFail()
{
	int h;
	int x = 0, y = 0;

	if (h  > 0) x = 1;
	if (x == 0) y = 1;
	x = 0;
	
	assert(is_low(x));
}

void nothingToDo()
{
	int h;
	int x = 0, y = 0, z = 0;
	
	if (h >0) x = 1;
	if (x == 0) y = 1;
	z = 1;
	x = 0;
}
