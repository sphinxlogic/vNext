#include <vcc.h>

typedef struct _ABS_LIST
{
	int Card;
	_(ghost \objset List)
} ABS_LIST, *PABS_LIST;

_(ghost _(pure) \bool IsEmptyAbsList(ABS_LIST AL) _(ensures \result == (AL.Card==0 && AL.List=={})))

void foo() {
  ABS_LIST l;
  _(assume IsEmptyAbsList(l))
}

`
Verification of foo succeeded.
`
