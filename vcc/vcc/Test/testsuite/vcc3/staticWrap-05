#include <vcc.h>

struct B { int x,y; };

struct A2 {
  int x;
  struct B b;

  _(invariant x ==> &b \in \this->\owns)
};

struct A3 {
  int x;
  struct B b;

  _(invariant x ==> &b \in0 \this->\owns)
};

struct A4 {
  int x;
  struct B b[5];

  _(invariant \forall int x; 0 <= x && x < 5 ==> &b[x] \in \this->\owns)
};
`
