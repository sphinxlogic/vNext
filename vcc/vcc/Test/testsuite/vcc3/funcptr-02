#include <vcc.h>

typedef int (*foo)(int i)
  _(requires i > 0)
  _(ensures \result > 0);

int test(int i)
  _(requires i > 0)
  _(ensures \result > 0)
{
  return i;
}

int bar(foo fum)
  _(reads fum)  _(requires fum->\valid)
{
  return fum(0);
}

int myn(){
  return bar((foo)&test);
}
`
