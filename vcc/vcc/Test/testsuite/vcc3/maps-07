typedef unsigned __int64 UINT64;

_(typedef \bool BITMAP[UINT64];)


_(ghost _(pure) BITMAP ToBitMap(UINT64 n);)


_(axiom \forall UINT64 x; (\forall UINT64 i; i < 64 ==> !ToBitMap(x)) ==> x == 0)
