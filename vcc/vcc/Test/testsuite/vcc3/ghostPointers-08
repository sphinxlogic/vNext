#include <vcc.h>

struct S {
  int a;
  _(ghost int b)
};

struct T {
  struct S s;
  _(ghost struct S r)
};

void foo(struct T *p) {
  _(ghost int ^i1 = &(p->s.b))
  _(ghost int ^i1 = &(p->r.a))
}

_(ghost void bar(struct T ^p) {
  int ^i1 = &(p->s.a);
})
`
Verification of foo succeeded.
Verification of bar succeeded.
`
