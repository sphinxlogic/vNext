#include <vcc.h>

_(pure) int f1(int *x)
  _(reads x);

_(pure) int f2(int *x)
  _(reads x);

_(pure) int f3(int *x)
  _(reads x);

_(pure) int f4(int *x)
  _(reads x);

_(pure) int f5(int *x)
  _(reads x);

_(pure) int f6(int *x)
  _(reads x);

#ifdef VERIFY
vcc_attr ("is_reads_check", "f1")
vcc_attr ("is_reads_check", "f2")
void reads_f12()
{
  _(reads_havoc)
}


vcc_attr ("is_reads_check", "f3")
void reads_f3_1()
{
  _(reads_havoc)
}

vcc_attr ("is_reads_check", "f3")
void reads_f3_2()
{
  _(reads_havoc)
}

vcc_attr ("is_reads_check", "f4")
void reads_f4(short *y)
{
  _(reads_havoc)
}

vcc_attr ("is_reads_check", "f5")
void reads_f5(int *x, short *y)
{
  _(reads_havoc)
}



vcc_attr ("is_reads_check", "szczebrzyszczykiewicz")
void reads_szczebrzyszczykiewicz()
{
  _(reads_havoc)
}
#endif
