// Old VCC keywords must not be recognized as such
void foo() {
  __specification
  __unchecked
  __invariant
  __decreases
  __requires
  __ensures
  __writes
  __lambda
  __forall
  __exists
  __assume
  __assert
  __reads
  __block
  __axiom
  __old p;
}

